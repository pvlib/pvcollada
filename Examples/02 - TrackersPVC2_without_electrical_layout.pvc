	    <technique profile="PVCollada-2.0"> 
          <!-- gap tag, child tags for illustrative purpose only -->		
          <gap>
		    <type>joint</type>
          </gap>
		</technique>
      </extra>
    </geometry>	
	<!-- We define the model for motor gaps (contains the geometry of the motor, no need to define it otherwise) -->
	<geometry id="MotorGapModel1">
      <mesh>
        <source id="motor_positions">
          <float_array id="motor_positions_array" count="24">
