          <technique_common>
            <accessor source="#motor_positions_array" count="8" stride="3">
              <param name="X" type="float"/>
              <param name="Y" type="float"/>
              <param name="Z" type="float"/>
            </accessor>
          </technique_common>
        </source>
        <vertices id="motor_vertices">
          <input semantic="POSITION" source="#motor_positions"/>
        </vertices>
        <polygons count="6">
          <input semantic="VERTEX" source="#motor_vertices" offset="0"/>
            <p>0 1 2 3</p>
            <p>4 5 6 7</p>
            <p>0 1 5 4</p>
            <p>1 2 6 5</p>
            <p>2 3 7 6</p>
            <p>3 0 4 7</p>
        </polygons>
      </mesh>
	  <extra>
	    <technique profile="PVCollada-2.0">  
