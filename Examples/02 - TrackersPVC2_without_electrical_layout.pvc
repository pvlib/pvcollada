        </source>
        <vertices id="cylinder_vertices">
          <input semantic="POSITION" source="#cylinder_positions"/>
        </vertices>
        <polygons count="10">
          <input semantic="VERTEX" source="#cylinder_vertices" offset="0"/>
          <p>0 1 2 3 4 5 6 7</p>
          <p>8 9 10 11 12 13 14 15</p>
          <p>0 1 9 8</p>
          <p>1 2 10 9</p>
          <p>2 3 11 10</p>
          <p>3 4 12 11</p>
          <p>4 5 13 12</p>
          <p>5 6 14 13</p>
          <p>6 7 15 14</p>
          <p>7 0 8 15</p>
        </polygons>
      </mesh>
